{
    "name": "robclancy/presenter",
    "description": "This is a simple library to help make a Presenter for your objects.",
    "keywords": ["laravel", "presenter", "decorator"],
    "license": "DBAD",
    "authors": [
        {
            "name": "Robbo",
            "email": "mail@robertclancy.net"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "illuminate/contracts": "5.*",
        "illuminate/support": "5.*",
        "illuminate/view": "5.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Robbo\\Presenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability": "stable"
}
